transfer_default
[ ! -d "$mountpoint/var/lib/clover/backgrounds" ] && mkdir -p "$mountpoint/var/lib/clover/backgrounds" && cp -rs "$squashfs/usr/share/backgrounds" "$mountpoint/var/lib/clover" && copy "$rootfs/backgrounds" "$mountpoint/var/lib/clover" 
rm -fr "$rootfs/backgrounds"

cfg_DefKorns_bordershack_enabled='y'
return 1
